{
    "parser": "babel-eslint",
    "env": {
        "es6": true
    },
    "plugins": [
        "react"
    ],

  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": ["airbnb"],
  "rules":{
    "react/no-unused-prop-types": [1],
    "react/prop-types": [1],
    "no-unused-vars": [1],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}